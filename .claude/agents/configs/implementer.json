{
  "name": "implementer",
  "description": "Writes code to implement features defined in specs",
  "prompt_file": "implementer.md",
  "allowed_tools": [
    "Read",
    "Grep",
    "Glob",
    "Bash",
    "Edit",
    "Write"
  ],
  "forbidden_tools": [],
  "mode": "implement",
  "allowed_paths": [
    "src/",
    "Assets/Scripts/",
    "tests/",
    "Assets/Tests/"
  ],
  "forbidden_paths": [
    ".claude/",
    ".git/",
    "Specs/",
    "node_modules/",
    ".env"
  ],
  "max_iterations": 20,
  "timeout_minutes": 30
}
